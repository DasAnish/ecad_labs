			rotary_event_1_rotary_ccw : out std_logic;                                        -- rotary_ccw
			rotary_event_2_rotary_cw  : out std_logic;                                        -- rotary_cw
			rotary_event_2_rotary_ccw : out std_logic;                                        -- rotary_ccw
